{
    "compilerOptions": {
      "target": "ES5",
      "lib": ["dom", "esnext"],
      "module": "ESNext",
      "moduleResolution": "node",
      "jsx": "react-jsx",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "strict": true,
      "baseUrl": ".",
      "paths":{
        "@/*": ["src/*"]
      }
    },
    "include": ["**/*.ts", "**/*.tsx"],
    "exclude": ["node_modules"]
  }
  